{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/js/(.*)$",
      "dest": "/js/$1"
    },
    {
      "src": "^/styles/(.*)$",
      "dest": "/styles/$1"
    },
    {
      "src": "^/icons/(.*)$",
      "dest": "/icons/$1"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/sw.js",
      "dest": "/sw.js"
    },
    {
      "src": "^/offline.html",
      "dest": "/offline.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 